    "@rollup/plugin-url": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.2.0",
    "lodash": "^4.17.15",
    "rollup": "^2.14.0",
    "rollup-plugin-postcss": "^3.1.2",
